<h2>All Users</h2>

    <% @users.each do |user| %>
    <ul>
        <li>
            <strong><%= user.username%></strong><br/>
            <%= user.email%><br/>
            <%= link_to 'Show', user %>
            <%= link_to 'Edit', edit_user_path(user) %>
        </li>
    </ul>
    <% end %>

<br>

<%= link_to 'New User', new_user_path %>

